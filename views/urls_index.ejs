<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp</title>
  <% include ./partials/_head %>
</head>
<body>
    <!-- Add your EJS code here -->

    <header>
        <% include ./partials/_header %>

    </header>

         <table>
          <tr>
            <th>Code</th>
            <th>Destination Address</th>
            <th>Edit</th>
            <th>Active Link</th>
            <th>Delete</th>
          </tr>

          <% Object.keys(abbLinks).forEach(function(key) { %>
          <tr>
            <td style="width:120px">u/<%= key %></td>
            <td style="width:300px"><%= abbLinks[key] %> </td>
            <td style="width:80px"><a href="/urls/<%=key%>" >Edit</a>
            <td style="width:200px"><a href="/u/<%=key%>" class="btn btn-info" style="width:170px">u/<%=key%></a></td>
            <td style="width:80px">
              <form method="POST" action="<%=`/urls/${key}/delete`%>" style="display:inline">
              <INPUT TYPE="SUBMIT" NAME="button" Value="Delete" class="btn btn-danger"/>
              </form></a>
            </td>
          </tr>
           <% });%>
        </table>


</body>
</html>